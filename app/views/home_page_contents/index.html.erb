<h1>Listing home_page_contents</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @home_page_contents.each do |home_page_content| %>
      <tr>
        <td><%= home_page_content.content %></td>
        <td><%= link_to 'Show', home_page_content %></td>
        <td><%= link_to 'Edit', edit_home_page_content_path(home_page_content) %></td>
        <td><%= link_to 'Destroy', home_page_content, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

